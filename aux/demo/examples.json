{
  "basic_commands": {
    "navigate": {
      "id": "nav_001",
      "method": "navigate",
      "session_id": "demo_session",
      "url": "https://example.com",
      "wait_until": "load",
      "timeout": 30000
    },
    "click": {
      "id": "click_001",
      "method": "click",
      "session_id": "demo_session",
      "selector": "button.submit",
      "button": "left",
      "click_count": 1,
      "timeout": 30000
    },
    "fill": {
      "id": "fill_001",
      "method": "fill",
      "session_id": "demo_session",
      "selector": "input[name='username']",
      "text": "demo_user",
      "clear_first": true,
      "press_enter": false,
      "timeout": 30000
    },
    "extract": {
      "id": "extract_001",
      "method": "extract",
      "session_id": "demo_session",
      "selector": "h1",
      "extract_type": "text",
      "multiple": false,
      "timeout": 30000
    },
    "wait": {
      "id": "wait_001",
      "method": "wait",
      "session_id": "demo_session",
      "selector": ".loading",
      "condition": "hidden",
      "timeout": 30000
    }
  },
  "advanced_commands": {
    "navigate_with_referer": {
      "id": "nav_002",
      "method": "navigate",
      "session_id": "demo_session",
