      "validate_input": true,
      "timeout": 30000
    },
    "extract_multiple_attributes": {
      "id": "extract_002",
      "method": "extract",
      "session_id": "demo_session",
      "selector": "a",
      "extract_type": "attribute",
      "attribute_name": "href",
      "multiple": true,
      "trim_whitespace": true,
      "timeout": 30000
    },
    "wait_for_text_content": {
      "id": "wait_002",
      "method": "wait",
      "session_id": "demo_session",
      "selector": ".status",
      "condition": "visible",
      "text_content": "Complete",
      "poll_interval_ms": 200,
      "timeout": 30000
    },
    "wait_with_custom_js": {
      "id": "wait_003",
      "method": "wait",
      "session_id": "demo_session",
      "condition": "load",
      "custom_js": "return document.readyState === 'complete' && window.jQuery !== undefined;",
      "poll_interval_ms": 100,
      "timeout": 30000
    }
  },
